                        <choice>
                              <value>high</value>
                              <value>medium</value>
                              <value>low</value>
                              <value>unknown</value>
                        </choice>
                  </attribute>
            </optional>
      </define>
      <define xmlns="http://relaxng.org/ns/structure/1.0" name="att.editLike.attribute.resp">
            <optional>
                  <attribute name="resp">
                        <a:documentation
                              xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
