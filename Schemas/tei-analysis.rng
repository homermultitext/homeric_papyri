                  <ref name="teicl"/>
                  <ref name="teiphr"/>
                  <ref name="teiw"/>
                  <ref name="teim"/>
                  <ref name="teic"/>
                  <ref name="teiseg"/>
            </choice>
      </define>
      <define xmlns="http://relaxng.org/ns/structure/1.0" name="teimodel.hiLike">
            <choice>
                  <ref name="teihi"/>
            </choice>
      </define>
      <define xmlns="http://relaxng.org/ns/structure/1.0" name="teimodel.emphLike">
            <choice>
                  <ref name="teiforeign"/>
                  <ref name="teiemph"/>
                  <ref name="teidistinct"/>
                  <ref name="teimentioned"/>
                  <ref name="teisoCalled"/>
                  <ref name="teigloss"/>
                  <!--<ref name="teiterm"/>-->
                  <ref name="teititle"/>
            </choice>
      </define>
      <define xmlns="http://relaxng.org/ns/structure/1.0" name="teimodel.highlighted">
            <choice>
                  <ref name="teimodel.hiLike"/>
                  <ref name="teimodel.emphLike"/>
            </choice>
      </define>
      <define xmlns="http://relaxng.org/ns/structure/1.0" name="teimodel.dateLike">
            <choice>
                  <ref name="teidate"/>
                  <ref name="teitime"/>
