            </element>
      </define>
      <define xmlns="http://relaxng.org/ns/structure/1.0" name="teiab">
            <element name="ab">
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >(anonymous block) contains any arbitrary component-level unit of text,
                        acting as an anonymous container for phrase or inter level elements
                        analogous to, but without the semantic baggage of, a paragraph.</a:documentation>
                  <ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="teimacro.paraContent"/>
                  <ref name="att.global.attribute.xmlid"/>
                  <ref name="att.global.attribute.n"/>
                  <ref name="att.global.attribute.xmllang"/>
                  <ref name="att.global.attribute.rend"/>
                  <ref name="att.global.attribute.rendition"/>
                  <ref name="att.global.attribute.xmlbase"/>
                  <ref name="att.global.linking.attribute.corresp"/>
                  <ref name="att.global.linking.attribute.synch"/>
                  <ref name="att.global.linking.attribute.sameAs"/>
                  <ref name="att.global.linking.attribute.copyOf"/>
                  <ref name="att.global.linking.attribute.next"/>
                  <ref name="att.global.linking.attribute.prev"/>
                  <ref name="att.global.linking.attribute.exclude"/>
                  <ref name="att.global.linking.attribute.select"/>
                  <ref name="att.global.analytic.attribute.ana"/>
                  <ref name="att.global.facs.attribute.facs"/>
                  <ref name="att.typed.attribute.type"/>
                  <ref name="att.typed.attribute.subtype"/>
                  <optional>
                        <attribute xmlns:a1="http://relaxng.org/ns/compatibility/annotations/1.0"
                              xmlns="http://relaxng.org/ns/structure/1.0" name="part"
                              a1:defaultValue="N">
                              <a1:documentation>specifies whether or not the block is complete.</a1:documentation>
                              <choice>
                                    <value>Y</value>
                                    <a1:documentation>(yes) the block is incomplete</a1:documentation>
                                    <value>N</value>
                                    <a1:documentation>(no) either the block is complete, or no claim
                                          is made as to its completeness</a1:documentation>
                                    <value>I</value>
                                    <a1:documentation>(initial) the initial part of an incomplete
                                          block</a1:documentation>
                                    <value>M</value>
                                    <a1:documentation>(medial) a medial part of an incomplete block</a1:documentation>
                                    <value>F</value>
                                    <a1:documentation>(final) the final part of an incomplete
                                    block</a1:documentation>
                              </choice>
                        </attribute>
                  </optional>
                  <empty/>
            </element>
      </define>
      <define xmlns="http://relaxng.org/ns/structure/1.0" name="teianchor">
