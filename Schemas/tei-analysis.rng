                                    <value>lines</value>
                                    <a:documentation
                                          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                          >lines of text</a:documentation>
                                    <value>chars</value>
                                    <a:documentation
                                          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                          >(characters) characters of text</a:documentation>
                                    <value>pages</value>
                                    <a:documentation
                                          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                          >pages, i.e. one side of a leaf</a:documentation>
                                    <value>cm</value>
                                    <a:documentation
                                          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                          >(centimetres) </a:documentation>
                                    <value>mm</value>
                                    <a:documentation
                                          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                          >(millimetres) </a:documentation>
                                    <value>in</value>
                                    <a:documentation
                                          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                          >(inches) </a:documentation>
                                    <data type="Name"/>
                              </choice>
                        </attribute>
                  </optional>
                  <empty/>
            </element>
